{
  "name": "electron-quick-start-flutter",
  "version": "1.0.0",
  "description": "A minimal Electron application with flutter & dart",
  "main": "debug.js",
  "scripts": {
    "clean": "flutter clean",
    "build:flutter": "flutter build web",
    "build:electron": "cd build/web && npm run build",
    "build:package":"cd build/web && npm run package",
    "flutter_server": "flutter run --web-port 8096 -d web-server",
    "dev": "run-p flutter_server start",
    "start": "electron .",
    "release": "run-s build:flutter build:electron",
    "package":"run-s build:flutter build:package"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "author": "GitHub",
  "license": "MIT",
  "devDependencies": {
    "electron": "^9.2.1",
    "electron-packager": "^15.0.0",
    "npm-run-all": "^4.1.5"
  }
}